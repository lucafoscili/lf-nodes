  "author": {
    "name": "Luca Foscili",
    "url": "https://www.lucafoscili.com"
  },
  "version": "2.2.0",
  "type": "module",
  "scripts": {
    "clean": "node ./web/src/scripts/clean.js",
    "build:scss": "node ./web/src/scripts/compile-scss.js",
    "build:ts": "tsc && tsc -p ./web/workflow-runner/tsconfig.json",
    "build:lfw": "node ./web/src/scripts/copy-widgets.js",
