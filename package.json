    "@types/jsdom": "^27",
    "@types/node": "^24.9.2",
    "fs-extra": "^11",
    "jsdom": "^27.1.0",
    "rollup-plugin-visualizer": "^5",
    "sass": "^1",
    "ts-node": "^10",
    "tsx": "^4",
    "typescript": "^5",
